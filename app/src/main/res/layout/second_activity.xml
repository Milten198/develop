<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#115500"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".SecondActivity">

    <EditText
        android:id="@+id/userName2"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#FFFFFF" />

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/userName2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="29dp"
        android:onClick="previousActivity"
        android:text="Previous" />

    <Button
        android:id="@+id/intercache"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:onClick="loadInternalCache"
        android:text="Load Internal Cache" />

    <Button
        android:id="@+id/extercache"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/intercache"
        android:layout_centerHorizontal="true"
        android:onClick="loadExternalCache"
        android:text="Load External Cache" />

    <Button
        android:id="@+id/exterpublic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/extercache"
        android:layout_centerHorizontal="true"
        android:onClick="loadExternalPublic"
        android:text="Load External Public" />

    <Button
        android:id="@+id/exterprivate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/exterpublic"
        android:layout_centerHorizontal="true"
        android:onClick="loadExternalPrivate"
        android:text="Load External Private" />

</RelativeLayout>